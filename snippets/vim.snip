snippet  save_cpoptions
abbr     let s:save_cpo = &cpo | set cpo&vim
alias    s:save_cpo cpoptions
options     head
    let s:save_cpo = &cpoptions
    set cpoptions&vim

    ${0}

    let &cpoptions = s:save_cpo
    unlet s:save_cpo

snippet  funca
abbr     autoload function
options     head
    function! `bufname('%') =~ '^autoload' ? join(split(bufname('%'), SpaceVim#api#import('file').separator)[1:-1], '#')[:-5] . '#' : ''`${1:fname}(${2:#:argv}) abort

    ${0}

    endfunction
